cache: cache.sqlite
stores:
  sqlite:
    type: sqlite
    cache: true
    configuration:
      database: dev.sqlite

scripts:
  upsert: configu upsert --store sqlite --set '' --schema cache.cfgu.json --config A1=222 --config A2=222
  eval: configu eval --store sqlite --set '' --schema cache.cfgu.json | configu export --explain
